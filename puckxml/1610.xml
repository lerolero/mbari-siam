					<RecordVariable columnIndex="42" format="double"
						longName="GPS Absolute Latitude" missingValue="-999" name="Latitude"
						units="degreesLatitude" validMax="90.0" validMin="-90.0">
						<description>latitude of GPS fix (signed fp degrees)</description>
						<StandardVariable name="latitude" namespaceUriString="http://marinemetadata.org/cf"></StandardVariable>
					</RecordVariable>
					<RecordVariable columnIndex="43" format="double"
						longName="GPS Absolute Longitude" missingValue="-999" name="Longitude"
						units="degreesLongitude" validMax="180.0" validMin="-180.0">
						<description>longitude of GPS fix (signed fp degrees)</description>
						<StandardVariable name="longitude" namespaceUriString="http://marinemetadata.org/cf"></StandardVariable>
					</RecordVariable>
					<RecordVariable columnIndex="44" format="integer"
						longName="UTC Minutes Since Sunday at 00:00:00" missingValue="-999"
						name="Minutes_Since_Sunday" units="minutes" validMax="10080" validMin="0">
						<description>Minutes since midnight on Sunday that GPS fix was
							obtained.</description>
					</RecordVariable>
				</RecordDescription>
			</DataStream>
		</output>
	</Deployment>
</Metadata>