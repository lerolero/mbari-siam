#
# Dial-up configuration for calling mbari via the globalstar modem
#
linkname globalstar
mooring:
ipparam mbari
#logfile /var/log/globalstar.log
#record /tmp/pppd.record
#debug
noauth
user M00r1nG~sVr
usepeerdns
maxconnect 3600         #disconnect after one hour
noipdefault
defaultroute
deflate 14
updetach

globalstar
38400
modem
crtscts
lock
connect /etc/ppp/dialMBARI
