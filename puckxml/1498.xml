<Metadata xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ssds.mbari.org/xml/schema/SSDS_Metadata.xsd"
	majorVersion="2" minorVersion="1" lastAuthor="$Author: mccann $"
	lastUpdate="$Date: 2008/11/19 19:37:10 $">
	<Deployment role="instrument" nominalDepth="1">
		<Device id="1498"/>
		<output>
			<DataStream>
				<RecordDescription bufferStyle="ASCII" bufferParseType="ordered"
					bufferItemSeparator="space" bufferLengthType="variable" parseable="true"
					recordType="1">
					<RecordVariable name="SensorModel" longName="SensorModel" units="string"
						columnIndex="1" format="string">
						<description>The model of the sensor reporting measurements</description>
					</RecordVariable>
					<RecordVariable name="SensorSerialNumber" longName="SensorSerialNumber"
						units="string" columnIndex="2" format="string">
						<description>The serial number of the sensor reporting
						measurements</description>
					</RecordVariable>
					<RecordVariable name="Oxygen" longName="Dissolved Oxygen" units="mmol/l"
						columnIndex="3" format="float">
						<description>Dissolved oxygen concentration in mmol per liter</description>
						<StandardVariable name="moles_of_oxygen_per_unit_volume_in_sea_water"/>
					</RecordVariable>

					<RecordVariable name="Air_Saturation" longName="Air Saturation" units="percent"
						columnIndex="4" format="float">
						<description>Air saturation value</description>
					</RecordVariable>
					<RecordVariable name="Temperature" longName="Temperature" units="deg C"
						columnIndex="5" format="float">
						<StandardVariable name="temperature_of_sensor_for_oxygen_in_sea_water"/>
					</RecordVariable>

				</RecordDescription>
			</DataStream>
		</output>
	</Deployment>
