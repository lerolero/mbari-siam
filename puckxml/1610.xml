						<description>Licor Air Pump Off - Humidity</description>
					</RecordVariable>
					<RecordVariable columnIndex="21" format="float" longName="" missingValue="-999"
						name="CTD_TMP" units="degrees C">
					</RecordVariable>
					<RecordVariable columnIndex="22" format="float" longName="" missingValue="-999"
						name="CTD_SAL" units="conductivity">
					</RecordVariable>
					<RecordVariable columnIndex="23" format="float" longName="" missingValue="-999"
						name="HR2_1" units="Volts">
					</RecordVariable>
					<RecordVariable columnIndex="24" format="float" longName="" missingValue="-999"
						name="HR2_2" units="Volts">
					</RecordVariable>
					<RecordVariable columnIndex="25" format="float" longName="" missingValue="-999"
						name="HR2_3" units="Volts">
					</RecordVariable>
					<RecordVariable columnIndex="26" format="float" longName="" missingValue="-999"
						name="HR2_4" units="Volts">
					</RecordVariable>
					<RecordVariable columnIndex="27" format="float" longName="" missingValue="-999"
						name="HR2_5" units="Volts">
					</RecordVariable>
					<RecordVariable columnIndex="28" format="float" longName="" missingValue="-999"
						name="HR2_6" units="Volts">
					</RecordVariable>
					<RecordVariable columnIndex="29" format="float" longName="" missingValue="-999"
						name="HR2_7" units="Volts">
					</RecordVariable>
					<RecordVariable columnIndex="30" format="float" longName="" missingValue="-999"
						name="HR2_8" units="Volts">
					</RecordVariable>
					<RecordVariable columnIndex="31" format="float" longName="" missingValue="-999"
						name="HR2_9" units="Volts">
					</RecordVariable>
					<RecordVariable columnIndex="32" format="float" longName="" missingValue="-999"
						name="HR2_10" units="Volts">
					</RecordVariable>
					<RecordVariable columnIndex="33" format="float" longName="" missingValue="-999"
						name="HR2_11" units="Volts">
