<Metadata xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://new-ssds.mbari.org/ssds-docs/xml/schema/SSDS_Metadata.xsd"
		majorVersion="1" minorVersion="1" lastAuthor="$Author: mccann $" lastUpdate="$Date: 2010/03/17 18:54:19 $">
	<Deployment role="instrument" nominalDepth="225">
		<Device id="1798"></Device>
		<output>
			<DataStream>
				<description>IM CTD data stream with pressure and oxygen</description>
				<RecordDescription bufferStyle="ASCII" bufferParseType="ordered"
						bufferItemSeparator="," bufferLengthType="variable" parseable="true"
						recordType="1">
					
					<RecordVariable name="SerialNumber" longName="Serial Number" units=""
							columnIndex="1" format="int">
					</RecordVariable>
					<RecordVariable name="Temperature" longName="Water Temperature" units="deg C"
							columnIndex="2" format="float">
					</RecordVariable>
					<RecordVariable name="Conductivity" longName="Conductivity" units="S/m"
							columnIndex="3" format="float">
					</RecordVariable>
					<RecordVariable name="Pressure" longName="Pressure" units="decibars"
							columnIndex="4" format="float">
					</RecordVariable>
					<RecordVariable name="OxygenVolts" longName="Oxygen sensor voltage" units="V"
							columnIndex="5" format="float">
					</RecordVariable>
					<RecordVariable name="DateTime" longName="DateTime of Sample" units=""
							columnIndex="6" format="string">
						<description>datetime in 'dd MMM yyyy hh:mm:ss' format</description>
					</RecordVariable>
					<RecordVariable name="Sigma-t" longName="UCSD sigma-t" units="kg/m^3"
							columnIndex="7" format="float">
					</RecordVariable>
					<RecordVariable name="BatteryVoltage" longName="Battery Voltage" units="V"
							columnIndex="8" format="float">
					</RecordVariable>
					<RecordVariable name="OperatingCurrent" longName="Operating Current" units="mA"
							columnIndex="9" format="float">
					</RecordVariable>
					<RecordVariable name="RecordNumber" longName="Record Number" units=""
							columnIndex="10" format="int">
					</RecordVariable>
				</RecordDescription>
			</DataStream>
		</output>
	</Deployment>
</Metadata>

