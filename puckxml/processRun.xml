<!--  
	  This is a sample XML description file for a software process.  
	  This file should help you describe your data set(s) so that SSDS can automatically plot and process it.
	  The file will also help describe the process, so that the relationship between data sets can be maintained.
	  
	  When using this file, you would replace any occurrence of 'instr' with your instrument name.
	  Other replacements will be needed throughout the file, and are hopefully reasonably apparent.
	  
	  This file assumes your data will be stored on the tornado share ssdsdata, in directory names
	  corresponding to those found at <http://ssdspub.mbari.org/ssdsdata/mooring/m0/_example>.
	  
	  We expect you will need some help in producing this document.  Please don't hesitate to contact SSDS staff for help.
 -->
<Metadata xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://new-ssds.mbari.org/ssds-docs/xml/schema/SSDS_Metadata.xsd"
	majorVersion="1" minorVersion="2" lastAuthor="$Author: mccann $"
	lastUpdate="$Date: 2007/11/06 21:17:35 $">
	<ProcessRun name="Template Process">
		<!--If you repeatedly run this process, you can include the time you ran it by specifying the following attributes above:
			startDate="2004-04-23T22:14:23Z" endDate="2004-04-23T22:16:50Z"
		This will require submitting the XML with each run; see SSDS staff for help with this. -->

		<Software name="TemplateProcessSoftware" softwareVersion="1.1"
			url="http://moonjelly.shore.mbari.org/cgi-bin/cvsweb.cgi/DPforSSDS/instr/sourceFileName.m?rev=1.1&amp;content-type=text/x-cvsweb-markup">
			<description>Software to do basic processing of instr data</description>
			<Person firstname="Your" surname="Name" organization="MBARI" email="you@mbari.org"
				phone="831-775-2063"/>
			<Resource name="instr Process XML"
				uri="http://moonjelly.shore.mbari.org/cgi-bin/cvsweb.cgi/DPforSSDS/instr/xml/instr_proc.xml?rev=1.1&amp;content-type=text/x-cvsweb-markup"
			/>
		</Software>

		<Resource name="calibfileA"
			uri="http://dods.mbari.org/data/ssdsdata/mooring/m0/instr/calib/calibFileA"
			resourceType="text/x-mbari-calib">
			<description>Calibration file</description>
		</Resource>
		<Resource name="procname_diary.log"
			uri="http://dods.mbari.org/data/ssdsdata/mooring/m0/instr/logs/procname_diary.log"
			resourceType="text/x-mbari-proclog">
			<description>Output log from the instr software process</description>
		</Resource>
		<Resource name="overview.png"
			uri="http://dods.mbari.org/data/ssdsdata/mooring/m0/instr/plots/overview.png"
			resourceType="image/png">
			<description>Overview plot of results</description>
		</Resource>

		<input>
			<!-- the first number after rawpackets is the instrument device ID, 
			the second is the metadata ID, the third is the record type, 
			and the fourth is the parent device ID.  See the SSDS team for help with this URL.-->
			<DataFile contentType="undefined"
				url="http://dods.mbari.org/data/rawpackets/1270_0_1_1299" dodsAccessible="true"
				name="Raw instr Data"/>
		</input>

		<output>
			<!-- A netcdf data file. (Note a special subdirectory exists for netcdf files. -->
			<DataFile contentType="application/x-mbari-data-netcdf"
				url="http://dods.mbari.org/data/ssdsdata/mooring/m0/instr/netcdf/fileNameA_mean.nc"
				dodsAccessible="true" name="fileNameA_mean.nc" original="false"/>

			<!-- Another netcdf data file.  As many of these can be named as you want. -->
			<DataFile contentType="application/x-mbari-data-netcdf"
				url="http://dods.mbari.org/data/ssdsdata/mooring/m0/instr/netcdf/fileNameA_stdev.nc"
				dodsAccessible="true" name="fileNameA_stdev.nc" original="false"/>

			<!-- An HTML page, for example autogenerated to present data -->
			<DataFile contentType="text/html"
				url="http://dods.mbari.org/data/ssdsdata/mooring/m0/instr/html/dataproducts.html"
				dodsAccessible="true" name="Products Page" original="false"/>

			<!-- An image file, for example a plot of a variable -->
			<DataFile contentType="image/png"
				url="http://dods.mbari.org/data/ssdsdata/mooring/m0/instr/plots/imageNameA_stdev.png"
				dodsAccessible="true" name="Image Name" original="false"/>

			<!-- A data file which needs to be described in order to be parsed/plotted by SSDS. -->
			<!-- Note the data file is in the data subdirectory, rather than the netcdf subdirectory. -->
			<DataFile contentType="text/x-mbari-data-csv"
				url="http://dods.mbari.org/data/ssdsdata/mooring/m0/instr/data/fileNameB.txt"
				dodsAccessible="true" name="CSV Data" original="false">
				<HeaderDescription>
					<CommentTag tagString="%"/>
					<!-- lines beginning with % at start of file will be ignored -->
				</HeaderDescription>

				<description>Processed data from your instr</description>
				<RecordDescription bufferStyle="ASCII" bufferParseType="ordered"
					bufferItemSeparator="," bufferLengthType="variable" parseable="true"
					recordType="1">
					<!-- Sample Record: 
			    	    %date_time,jday,ecobbcal
			    	    26-May-2004 00:11:26,732093.007940,0.000953
			      	-->
					<RecordVariable name="date_time" longName="Date of sample" referenceScale="TBD"
						columnIndex="1" format="string">
						<description>date in dd-mmm-yyyy hh:mm:ss</description>
					</RecordVariable>
					<RecordVariable name="Jday" longName="Day of year" referenceScale="TBD"
						columnIndex="5" format="int">
						<description>Day of year of sample</description>
					</RecordVariable>
					<RecordVariable name="Total backscatter"
						longName="Calibrated total volume scattering coefficients" units="m-1"
						columnIndex="7" format="float">
						<description>backscatter with calibations applied,Bb(660)</description>
					</RecordVariable>
				</RecordDescription>
			</DataFile>
		</output>
	</ProcessRun>
</Metadata>
