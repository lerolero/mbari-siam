					</RecordVariable>
					<RecordVariable columnIndex="7" format="integer"
						longName="Licor Equilibrator Pump Off - CO2" missingValue="-999"
						name="Licor_Equil_Off_CO2" units="counts_times_10">
						<description>Licor Equilibrator Pump Off - CO2</description>
					</RecordVariable>
					<RecordVariable columnIndex="8" format="double"
						longName="Licor Equilibrator Pump Off - Press" missingValue="-999"
						name="Licor_Equil_Off_Press" units="counts">
						<description>Licor Equilibrator Pump Off - Press</description>
					</RecordVariable>
					<RecordVariable columnIndex="9" format="double"
						longName="Licor Equilibrator Pump Off - O2" missingValue="-999"
						name="Licor_Equil_Off_O2" units="counts_times_10">
						<description>Licor Equilibrator Pump Off - O2</description>
					</RecordVariable>
					<RecordVariable columnIndex="10" format="double"
						longName="Licor Equilibrator Pump Off - Humidity" missingValue="-999"
						name="Licor_Equil_Off_Hum" units="counts_times_10">
						<description>Licor Equilibrator Pump Off - Humidity</description>
					</RecordVariable>
					<RecordVariable columnIndex="11" format="integer"
						longName="Licor Zero Pump On - CO2" missingValue="-999"
						name="Licor_Zero_On_CO2" units="counts_times_10">
						<description>Licor Zero Pump On - CO2</description>
					</RecordVariable>
					<RecordVariable columnIndex="12" format="double"
						longName="Licor Zero Pump On - Press" missingValue="-999"
						name="Licor_Zero_On_Press" units="counts">
						<description>Licor Zero Pump On - Press</description>
					</RecordVariable>
					<RecordVariable columnIndex="13" format="integer"
						longName="Licor Zero Pump Off - CO2" missingValue="-999"
						name="Licor_Zero_Off_CO2" units="counts_times_10">
						<description>Licor Zero Pump Off - CO2</description>
					</RecordVariable>
					<RecordVariable columnIndex="14" format="double"
						longName="Licor Zero Pump Off - Press" missingValue="-999"
						name="Licor_Zero_Off_Press" units="counts">
						<description>Licor Zero Pump Off - Press</description>
					</RecordVariable>
					<RecordVariable columnIndex="15" format="integer"
						longName="Licor Air Pump On - CO2" missingValue="-999"
						name="Licor_Air_On_CO2" units="counts_times_10">
						<description>Licor Air Pump On - CO2</description>
					</RecordVariable>
					<RecordVariable columnIndex="16" format="double"
						longName="Licor Air Pump On - Press" missingValue="-999"
						name="Licor_Air_On_Press" units="counts">
						<description>Licor Air Pump On - Press</description>
					</RecordVariable>
					<RecordVariable columnIndex="17" format="integer"
						longName="Licor Air Pump Off - CO2" missingValue="-999"
						name="Licor_Air_Off_CO2" units="counts_times_10">
						<description>Licor Air Pump Off - CO2</description>
					</RecordVariable>
					<RecordVariable columnIndex="18" format="double"
						longName="Licor Air Pump Off - Press" missingValue="-999"
						name="Licor_Air_Off_Press" units="counts">
						<description>Licor Air Pump Off - Press</description>
					</RecordVariable>
					<RecordVariable columnIndex="19" format="double"
						longName="Licor Air Pump Off - O2" missingValue="-999"
						name="Licor_Air_Off_O2" units="counts_times_10">
						<description>Licor Air Pump Off - O2</description>
					</RecordVariable>
