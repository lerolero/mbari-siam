#
# Dial-up configuration for calling mbari via the short haul (direct) modem
#
linkname longhaul
ipparam mbari
shore:
logfile /var/log/longhaul.log
#record /tmp/pppd.record
debug
noauth
#user ops
#usepeerdns
#maxconnect 3600         #disconnect after one hour
noipdefault
#defaultroute
deflate 14
#noccp	#disable compression negotiation with the proprietary ADUS server
updetach

ttyS0
57600
modem
#local
crtscts
lock
passive
#persist
lcp-echo-interval 5
lcp-echo-failure 3
proxyarp
