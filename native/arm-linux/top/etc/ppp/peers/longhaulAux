#
# Dial-up configuration for calling mbari via the long haul (direct) modem
#
linkname longhaul
mooringAux:
ipparam mbari
#logfile /var/log/longhaul.log
#record /tmp/pppd.record
#debug
noauth
#user ops
#usepeerdns
#maxconnect 3600         #disconnect after one hour
noipdefault
defaultroute
deflate 14
#noccp	#disable compression negotiation with the proprietary ADUS server
updetach

ttySX12
57600
#modem
local
crtscts
lock
lcp-echo-interval 5
lcp-echo-failure 3
connect "/etc/siam/rfpower.ops 1 on"
disconnect "/etc/siam/rfpower.ops 1 off"
