					<RecordVariable columnIndex="37" format="integer" longName=""
						missingValue="-999" name="HS2" units="Counts">
					</RecordVariable>
					<RecordVariable columnIndex="38" format="integer" longName=""
						missingValue="-999" name="HS3" units="Counts">
					</RecordVariable>
					<RecordVariable columnIndex="39" format="integer" longName=""
						missingValue="-999" name="HS4" units="Counts">
