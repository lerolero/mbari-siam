<Metadata xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ssds.mbari.org/xml/schema/SSDS_Metadata.xsd"
	majorVersion="1" minorVersion="3" 
	lastAuthor="$Author: kgomes $" lastUpdate="$Date: 2006/11/29 16:59:38 $" >
	
	<Deployment role="instrument" >
		
		<Device	id="1537" name="Combination Turbidity and Fluorescence Sensor" 
			type="Backscatterometer" 
			mfgName="Wetlabs" mfgModel="ECO Triplet" mfgSerialNumber="WL-30011">
			<description>Wetlabs ECO Triplet three-optical-sensor instrument</description>
			<Person	firstname="Tom" surname="Oreilly" organization="MBARI"
				email="oreilly@mbari.org" />
		</Device>
		<output>
			<DataStream>
				<description>Wetlabs ECO Triplet data stream</description>
				
				
				<!-- Sample Record:
					04/28/06	07:29:32	5637	10135	17154
					04/28/06	07:29:33	17373	10135	17154
					04/28/06	07:29:33	17373	10135	17154
					
					Date Time 470 ref 470 sig 532 ref 532 sig 660 ref 660 sig therm
				-->
				
				<RecordDescription bufferStyle="ascii" 
					bufferLengthType="fixed"
					parseable="true"
					recordType="1"
					bufferParseType="ordered"
					bufferItemSeparator=" ">
					<RecordVariable
						name="Date_1"
						longName="Date of Sample 1"
						units="dd_mm_yy"	
						columnIndex="1"
						format="string"	>
						<description>date in dd/mm/yy format of 1st sample in this record</description>
						<!--StandardVariable name="date"/-->
					</RecordVariable>
					<RecordVariable
						name="Time_1"
						longName="Time of Day 1"
						units="hh_mm_ss"	
						columnIndex="2"
						format="string"	>
						<description>time of day in hh:mm:ss format of 1st sample in this record</description>
						<!--StandardVariable name="time"/-->
					</RecordVariable>
					<RecordVariable
						name="SignalA_1"
						longName="Signal A Sample 1"
						units="counts"	
						columnIndex="3"
						format="int" >
						<description>A signal of 1st sample in this record</description>
						<!--StandardVariable name="intensity"/-->
					</RecordVariable>
					<RecordVariable
						name="SignalB_1"
						longName="Signal B Sample 1"
						units="counts"	
						columnIndex="4"
						format="int" >
						<description>B signal of 1st sample in this record</description>
						<!--StandardVariable name="intensity"/-->
					</RecordVariable>
					<RecordVariable
						name="SignalC_1"
						longName="Signal C Sample 1"
						units="counts"	
						columnIndex="5"
						format="int" >
						<description>C signal of 1st sample in this record</description>
						<!--StandardVariable name="temperature"/-->
					</RecordVariable>
					
					<RecordVariable
						name="Date_2"
						longName="Date of Sample 2"
						units="dd_mm_yy"	
						columnIndex="6"
						format="string"	>
						<description>date in dd/mm/yy format of 2nd sample in this record</description>
						<!--StandardVariable name="date"/-->
					</RecordVariable>
					<RecordVariable
						name="Time_2"
						longName="Time of Day 2"
						units="hh_mm_ss"	
						columnIndex="7"
						format="string"	>
						<description>time of day in hh:mm:ss format of 2nd sample in this record</description>
						<!--StandardVariable name="time"/-->
					</RecordVariable>
					<RecordVariable
						name="SignalA_2"
						longName="Signal A Sample 2"
						units="counts"	
						columnIndex="8"
						format="int" >
						<description>A signal of 2nd sample in this record</description>
						<!--StandardVariable name="intensity"/-->
					</RecordVariable>
					<RecordVariable
						name="SignalB_2"
						longName="Signal B Sample 2"
						units="counts"	
						columnIndex="9"
						format="int" >
						<description>B signal of 2nd sample in this record</description>
						<!--StandardVariable name="intensity"/-->
					</RecordVariable>
					<RecordVariable
						name="SignalC_2"
						longName="Signal C Sample 2"
						units="counts"	
						columnIndex="10"
						format="int" >
						<description>C signal of 2nd sample in this record</description>
						<!--StandardVariable name="temperature"/-->
					</RecordVariable>
					
					<RecordVariable
						name="Date_3"
						longName="Date of Sample 3"
						units="dd_mm_yy"	
						columnIndex="11"
						format="string"	>
						<description>date in dd/mm/yy format of 3rd sample in this record</description>
						<!--StandardVariable name="date"/-->
					</RecordVariable>
					<RecordVariable
						name="Time_3"
						longName="Time of Day 3"
						units="hh_mm_ss"	
						columnIndex="12"
						format="string"	>
						<description>time of day in hh:mm:ss format of 3rd sample in this record</description>
						<!--StandardVariable name="time"/-->
					</RecordVariable>
					<RecordVariable
						name="SignalA_3"
						longName="Signal A Sample 3"
						units="counts"	
						columnIndex="13"
						format="int" >
						<description>A signal of 3rd sample in this record</description>
						<StandardVariable name="intensity"/>
					</RecordVariable>
					<RecordVariable
						name="SignalB_3"
						longName="Signal B Sample 3"
						units="counts"	
						columnIndex="14"
						format="int" >
						<description>B signal of 3rd sample in this record</description>
						<StandardVariable name="intensity"/>
					</RecordVariable>
					<RecordVariable
						name="SignalC_3"
						longName="Signal C Sample 3"
						units="counts"	
						columnIndex="15"
						format="int" >
						<description>C signal of 3rd sample in this record</description>
						<StandardVariable name="temperature"/>
					</RecordVariable>
					
				</RecordDescription>
			</DataStream>
		</output>
		<output>
			<DataStream>
				<description>Summary Wetlabs ECO Triplet Data Stream</description>
				<RecordDescription bufferStyle="ascii" 
					bufferLengthType="variable" 
					parseable="false" 
					recordType="1000">
					<RecordVariable 
						name="ECO_Triplet_summary" 
						longName="Summary ECO Triplet Data Record" 
						units="noUnits" 
						columnIndex="1" >
						<description>Summary packets for ECO Triplet variables in condensed XML format. Contents for each variable: 
							number of samples, average, max, min, and standard deviation.</description>
					</RecordVariable>
				</RecordDescription>
			</DataStream>
		</output>
	</Deployment>
</Metadata>
